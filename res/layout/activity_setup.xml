<!--                     android:text="Upload logs" -->
<!--                     android:textColor="@color/black" -->
<!--                     android:textSize="18dp" /> -->
<!--             </TableRow> -->
        </TableLayout>
        <!-- The buttons at the bottom of the screen. -->

    </ScrollView>

</LinearLayout>